<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/md_theme_onPrimaryFixed">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/frameLayout2"
        android:layout_width="0dp"
        android:layout_height="78dp"
        android:background="@color/md_theme_onSecondaryFixed"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="60dp"
            android:layout_height="60dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/cat"
            app:tint="#878787" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="311dp"
        android:layout_height="311dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout2"
        app:srcCompat="@drawable/register" />

    <EditText
        android:id="@+id/JTFnome"
        android:layout_width="294dp"
        android:layout_height="51dp"
        android:layout_marginTop="48dp"
        android:background="@color/md_theme_onPrimaryFixedVariant"
        android:ems="10"
        android:hint="nome"
        android:inputType="text"
        android:textAlignment="center"
        android:textColorHint="@color/md_theme_onBackground_mediumContrast"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <EditText
        android:id="@+id/JTFpassword"
        android:layout_width="294dp"
        android:layout_height="51dp"
        android:layout_marginTop="16dp"
        android:background="@color/md_theme_onPrimaryFixedVariant"
        android:ems="10"
        android:hint="Senha"
        android:inputType="textPassword"
        android:textAlignment="center"
        android:textColorHint="@color/md_theme_onBackground_mediumContrast"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/JTFEmail" />

    <Button
        android:id="@+id/JBlogin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Cadastrar"
        app:layout_constraintEnd_toEndOf="@+id/JTFconfirmPassword"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/JTFconfirmPassword"
        app:layout_constraintTop_toBottomOf="@+id/JTFconfirmPassword" />

    <EditText
        android:id="@+id/JTFEmail"
        android:layout_width="294dp"
        android:layout_height="51dp"
        android:layout_marginTop="16dp"
        android:background="@color/md_theme_onPrimaryFixedVariant"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:textAlignment="center"
        android:textColorHint="@color/md_theme_onBackground_mediumContrast"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/JTFnome" />

    <EditText
        android:id="@+id/JTFconfirmPassword"
        android:layout_width="294dp"
        android:layout_height="51dp"
        android:layout_marginTop="16dp"
        android:background="@color/md_theme_onPrimaryFixedVariant"
        android:ems="10"
        android:hint="Senha"
        android:inputType="textPassword"
        android:textAlignment="center"
        android:textColorHint="@color/md_theme_onBackground_mediumContrast"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/JTFpassword" />

</androidx.constraintlayout.widget.ConstraintLayout>